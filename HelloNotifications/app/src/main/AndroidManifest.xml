<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="marctan.com.hellonotifications" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="marctan.com.hellonotifications.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="marctan.com.hellonotifications.ContentActivity"
            android:label="@string/title_activity_content" >
        </activity>
        <activity
            android:name="marctan.com.hellonotifications.ReplyActivity"
            android:label="@string/title_activity_reply" >
        </activity>
    </application>

</manifest>
